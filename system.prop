#
# system.prop for n7000
#

vendor.rild.libpath=/vendor/lib/libsecril-shim.so
vendor.rild.libargs=-d /dev/ttyS0
ro.ril.telephony.mqanelements=5
ro.ril.telephony.nstrings=4

# EGL blobs crash on screen off
ro.egl.destroy_after_detach=true

# Disable colorfade, which can cause crash when sleeping
ro.displaypower.colorfade=false

persist.logcat.enabled=0

ro.config.low_ram=1
